{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "paths": {
            "#/*": [
                "./src/*"
            ],
            "##/*": [
                "./auto/src/*"
            ],
            "&/*": [
                "./assets/tsmodule/*"
            ],
            "&&/*": [
                "./auto/assets/tsmodule/*"
            ]
        }
    },
    "include": [
        "../../src/**/*"
    ]
}
